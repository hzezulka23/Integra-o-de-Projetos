<script>
var IDPASTACLICADA = 1;
var PASTACRIADAS = 0;
window.onload = function() {
}
function InputAppers(id){
var Titulo = id.replace('Divform','');
document.getElementById("TitulosDasPastas"+Titulo).innerHTML = "";
var elements = document.getElementsByClassName('Divform');
for (var i = 0; i < elements.length; i++) {
elements[i].style.display = 'none';
}
document.getElementById(id).style.display ='block';
IDPASTACLICADA = id.replace('Divform','');
}

function InputDesppers(id,id2){
document.getElementById(id).style.display ='none';
document.getElementById(id2).HTML ='uhum';
}
function Sumir() {
document.getElementById('div1').style.display = 'none';
	
}

$('.input').on('keyup', function (e) {
    if (e.keyCode == 13) {
        document.getElementById('TitulosDasPastas'+String(IDPASTACLICADA)).innerHTML = document.getElementById("input"+String(IDPASTACLICADA)).value;
        document.getElementById('Divform'+String(IDPASTACLICADA)).style.display ='none';
        document.getElementById("HFolderOptions"+String(PASTACRIADAS+1)).innerHTML = document.getElementById("input"+String(IDPASTACLICADA)).value;
    	  InserirNovasPastas(1,document.getElementById("input"+String(IDPASTACLICADA)).value);	
    }
});

function CreateNewFolder() {
	if(PASTACRIADAS == 10){alert("Máximo de 10 Pastas")
	}else{
	PASTACRIADAS = PASTACRIADAS +1;	
	document.getElementById("BackFolder"+String(PASTACRIADAS)).style.display = 'block';
	document.getElementById("FolderOptions"+String(PASTACRIADAS+1)).style.display = 'block';
	//DeletarPasta(1,"Folder");
	//PastasDisponiveis(1);
	for(var k = 0;k<PASTACRIADAS;k++){
	MudarPosicao("BackFolder"+String(k+1),PASTACRIADAS-k-1);		
	}} 
		
}

function MudarPosicao(id,k) {
var Left = [43.2,5,43.2,5,43.2,5,43.2,5,43.2,5];
var Top = [4,35.5,35.5,66.7,66.7,98,98,129.5,129.5,161];
document.getElementById(id).style.marginLeft = String(Left[k]) + "%";
document.getElementById(id).style.marginTop = String(Top[k]) + "%";		
} 

</script>	

