function temp(classe) 
{
	var tamanho = (BLAZER.length);
	if(classe==1){		
		var index = Math.floor((Math.random()*(tamanho)));
		document.getElementById("ImgExp1F1").src	= BLAZER[index];
		PegarTamanho(BLAZER[index],"ExpF1");
		index = Math.floor((Math.random()*(tamanho)));
		document.getElementById("ImgExp1F2").src	= BLAZER[index];
		PegarTamanho(BLAZER[index],"ExpF2");
		index = Math.floor((Math.random()*(tamanho)));
		document.getElementById("ImgExp1F3").src	= BLAZER[index];
		PegarTamanho(BLAZER[index],"ExpF3");
		index = Math.floor((Math.random()*(tamanho)));
		document.getElementById("ImgExp1F4").src	= BLAZER[index];
		PegarTamanho(BLAZER[index],"ExpF4");
		index = Math.floor((Math.random()*(tamanho)));
		document.getElementById("ImgExp1F5").src	= BLAZER[index];
		PegarTamanho(BLAZER[index],"ExpF5");
		index = Math.floor((Math.random()*(tamanho)));
		document.getElementById("ImgExp2F1").src	= BLAZER[index];
		index = Math.floor((Math.random()*(tamanho)));
		document.getElementById("ImgExp2F2").src	= BLAZER[index];
		index = Math.floor((Math.random()*(tamanho)));
		document.getElementById("ImgExp2F3").src	= BLAZER[index];
		index = Math.floor((Math.random()*(tamanho)));
		document.getElementById("ImgExp2F4").src	= BLAZER[index];
		index = Math.floor((Math.random()*(tamanho)));
		document.getElementById("ImgExp2F5").src	= BLAZER[index];
		index = Math.floor((Math.random()*(tamanho)));
		document.getElementById("ImgExp3F1").src	= BLAZER[index];
		index = Math.floor((Math.random()*(tamanho)));
		document.getElementById("ImgExp3F2").src	= BLAZER[index];
		index = Math.floor((Math.random()*(tamanho)));
		document.getElementById("ImgExp3F3").src	= BLAZER[index];
		index = Math.floor((Math.random()*(tamanho)));
		document.getElementById("ImgExp3F4").src	= BLAZER[index];
		index = Math.floor((Math.random()*(tamanho)));
		document.getElementById("ImgExp3F5").src	= BLAZER[index];
		
	}
}

function CamadaExtra() {
	var tamanho = (BLAZER.length);	
	var index = Math.floor((Math.random()*(tamanho)));
	document.getElementById("imgextra1").src	= BLAZER[index];
	var tamanho = (MANGA.length);	
	var index = Math.floor((Math.random()*(tamanho)));
	document.getElementById("imgextra2").src	= MANGA[index];
	var tamanho = (COSTAS.length);	
	var index = Math.floor((Math.random()*(tamanho)));
	document.getElementById("imgextra3").src	= COSTAS[index];
	var tamanho = (JEANS.length);	
	var index = Math.floor((Math.random()*(tamanho)));
	document.getElementById("imgextra5").src	= JEANS[index];
	var tamanho = (SHORTS.length);	
	var index = Math.floor((Math.random()*(tamanho)));
	document.getElementById("imgextra4").src	= SHORTS[index];

}